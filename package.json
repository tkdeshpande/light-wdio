{
  "name": "docker-wdio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start-hub": "docker-compose down && docker-compose up -d",
    "start-hub-debug": "docker-compose down && docker-compose -f docker-compose-debug.yml up -d",
    "test": "npm run start-hub && wdio wdio.conf.js",
    "test-debug": "npm run start-hub-debug && wdio run wdio.conf.debug.js",
    "generateMochawesome": "node mergeResults.js && marge Results/wdio-ma-merged.json --reportTitle 'My Project Results'",
    "report-cleanup": "rm -rf Results mochawesome-report reports/html-reports/screenshots/*"
  },
